{
  "name": "ucl-syntax-highlighting",
  "displayName": "UCL (Rspamd)",
  "description": "Syntax highlighting for UCL, a configuration language primarily used for Rspamd",
  "version": "1.0.1",
  "publisher": "louis-lau",
  "repository": {
    "type": "git",
    "url": "https://github.com/louis-lau/vscode-ucl-syntax-highlighting.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [
    "ucl",
    "rspamd",
    "syntax",
    "highlighting"
  ],
  "scripts": {
    "convert-grammar": "npx js-yaml syntaxes/ucl.tmLanguage.yml > syntaxes/ucl.tmLanguage.json",
    "pretest": "npm run convert-grammar",
    "test": "npx vscode-tmgrammar-test -c -g syntaxes/ucl.tmLanguage.json 'tests/**/*.test.ucl'"
  },
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ucl",
        "aliases": [
          "UCL",
          "ucl",
          "rspamd"
        ],
        "extensions": [
          ".conf",
          ".ucl",
          ".ucl.conf"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ucl",
        "scopeName": "source.ucl",
        "path": "./syntaxes/ucl.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.0",
    "vscode-tmgrammar-test": "^0.1.3"
  }
}
